@model HomeIndexViewModel



 <div class="slider-thumb">
    <div class="container mt-5">
    <div class="row align-items-center">
        <div class="col-md-6">
            <span class="display-4">¡Hola, soy @Model.Persona.Nombre!</span>
            <h5>Desarrollador Fullstack enfocado en tecnologías .Net y JavaScript</h5>
        </div>
        <div class="col-md-6 text-center">
            <img id="photo" class="img-fluid mt-4" src="~/images/myself.png" alt="Foto de Yerson" />
        </div>
    </div>
</div>


        <div class="container mt-5">
            <div class="row align-items-center text-center">
                <div class="col">
                    <a href="#about-me" class="nav-link">Sobre mi</a>
                    <a href="#experience" class="nav-link">Experiencia</a>
                    @if (Model.Proyectos is not null)
                    {
                        <a href="#projects" class="nav-link">Proyectos</a>
                    }
                    <a href="#contact" class="nav-link">Contacto</a>
                </div>
            </div>
        </div>
<section id="about-me">
    <div class="row align-items-center">
    <div class="row justify-content-center text-center mb-5">
        <div class="col-md-8">
            <h1>@Model.Persona.Titulo</h1>
            <p class="lead">
                @Model.Persona.Descripcion
            </p>

        </div>
    </div>
</div>
    </section>
</div>







